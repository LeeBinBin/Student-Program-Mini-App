/* 性能优化：减少重绘 */
.grid-item, .feature-item {
  will-change: transform, box-shadow;
}

/* 触摸反馈 */
.grid-item {
  -webkit-tap-highlight-color: transparent;
}

.grid-item:active {
  transform: translateY(-2px);
  box-shadow: 0 6px 16px rgba(0,0,0,0.1);
}

